<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	</ui:style>
	<g:VerticalPanel ui:field="mainPanel" styleName="panelSVC">
		<g:FocusPanel ui:field="headerPanel">
		<g:HorizontalPanel ui:field="headerHp" styleName="headerPanel">
			<g:HorizontalPanel>
				<g:Label ui:field="header" styleName="headerText"/>
				<g:Label/>
				<g:Label ui:field="headerKey" styleName="headerDataKey"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel styleName="buttonGroup">
				<g:Image ui:field="reloadIcon" url="images/icons/icon_refresh.png"  styleName="icon"/>
				<g:Image ui:field="minimizeIcon" url="images/icons/icon_min.png"  styleName="icon"/>
				<g:Image ui:field="closeBtn" url="images/icons/icon_close.png"  styleName="icon"/>
			</g:HorizontalPanel>
		</g:HorizontalPanel>
		</g:FocusPanel>
		<g:ScrollPanel ui:field="contentPanel"/>
	</g:VerticalPanel>
</ui:UiBinder> 